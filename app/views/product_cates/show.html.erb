<% title [@product_cate.name, "成都保险，保险咨询网"].join(' - ') %>
<% meta_keywords [@product_cate.name, "保险产品", "成都保险", "商业保险咨询"].join(' - ') %>
<% meta_description [@product_cate.name, "保险，成都保险，投保， 保险咨询，保险购买，商业保险，中国平安，咨询QQ：29928649, 专业团队为你提供保险咨询办理服务，聪明选择，受益终生！"].join(' - ') %>
<div class="product">
<h1>保险产品: <%= link_to @product_cate.name, product_cate_path(@product_cate) %></h1>

<ul>
  <% @product_items.each do |item| %>
    <li><%= link_to item.title, product_item_path(item), :target => '_blank' %></li>
  <% end %>
</ul>

<br />
<%= will_paginate @product_items %>
</div>